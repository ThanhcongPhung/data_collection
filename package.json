{
  "name": "react-boiler-plate",
  "version": "1.0.0",
  "description": "react boiler plate",
  "main": "index.js",
  "engine": {
    "node": "10.16.0",
    "npm": "6.9.0"
  },
  "scripts": {
    "start": "node server/index.js",
    "backend": "nodemon server/index.js",
    "frontend": "npm run start --prefix client",
    "dev": "concurrently \"npm run backend\" \"npm run start --prefix client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCT=false npm install --prefix client && npm run build --prefix client "
  },
  "author": "John ahn",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.838.0",
    "axios": "^0.21.1",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "file-type": "^16.1.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "multer": "^1.4.2",
    "request": "^2.88.2",
    "saslprep": "^1.0.3",
    "socket.io": "^3.0.4",
    "supports-color": "^7.1.0",
    "tmp": "^0.2.1",
    "web-push": "^3.4.4"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.0.0-rc.0",
    "concurrently": "^4.1.0",
    "nodemon": "^1.19.1"
  }
}
